QWidget {
background: #333;
color: white;
}

QMenuBar::item {
spacing: 3px;
padding: 1px 4px;
background: #333;
border-radius: 4px;
}

QMenuBar::item:selected {
background: #a8a8a8;
}

QMenuBar::item:pressed {
background: #888888;
}

QToolBar {
background: #1E1E1E;/*151211;*//*2E2E2E;*/
/*
background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
stop: 0 #2E2E2E, stop: 1.0 /*#2E2E2E#404040/*#585858);*/
border: 1px solid transparent;
}

QToolBar::separator {
background: #111;
width: 1px; /* when vertical */
height: 1px; /* when horizontal */
}

QToolBar#custom {
border-radius: 3px;
border: 1px solid #222;
}

QToolButton {
background: none;
}

QToolButton::hover {
border: none;
}

QScrollBar:vertical{
/*
background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
stop: 0 #212121,stop: 1.0 #323232);
*/
border: 2px solid #161616;
background: #161616;
width: 18px; /*18*/
}
QScrollBar:horizontal{
/*
background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
stop: 0 #212121,stop: 1.0 #323232);
*/
border: 2px solid #161616;
background: #161616;
height: 18px;
}
QScrollBar::handle:vertical{
padding: 2px;
min-height: 20px;
/*
background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
stop: 0 #585858,stop: 1.0 #404040);
*/
background: #131212;
border-radius: 7px; /*7*/
border: 1px solid #5B5A5A; /*#191919;*/
}

QScrollBar::handle:horizontal{
padding: 2px;
min-width: 20px;
/*
background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
stop: 0 #585858,stop: 1.0 #404040);
*/
background: #131212;
border-radius: 7px;
border: 1px solid #5B5A5A; /*#191919;*/
}

QScrollBar::corner{
background: #161616;
}

QTabBar::tear {
image: url(tear_indicator.png);
}

QTabBar::scroller { /* the width of the scroll buttons */
width: 20px;
}

QTabBar QToolButton { /* the scroll buttons are tool buttons */
/*border-image: url(":/Images/") 2; /***************************************************************************/
border-width: 2px;
}

QTabBar QToolButton::right-arrow { /* the arrow mark in the tool buttons */
image: url(":/Images/tab_right.png");
}

QTabBar QToolButton::left-arrow {
image: url(":/Images/tab_left.png");
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical,
QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical,
QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal,
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal{
background: none;
border: none;
}

QTabWidget::pane {
border-color: #555;
border-style: solid;
border-width: 1px;
background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
stop: 0 #60615f, stop: 1.0 #4a4b45);
position: absolute;
top: 2px;
}

QTabBar {
background-color: #333;
border: none;
}
QTabBar::close-button {
subcontrol-position: right;
}

QTabBar::tab {
color: #bbb;
padding: 4px 8px;/****************************/
border: 1px solid #222;
background: #252525;
border-radius: 10px;
/*
border-image: url(":/Images/tab.png");
*/
}
QTabBar::tab:top,
QTabBar::tab:bottom {
padding: 3px 4px; /*6, 8*/
border-right: 2px solid #111;
}
QTabBar::tab:left,
QTabBar::tab:right {
padding: 4px 3px;/*8, 6*/
border-top: 1px solid #111;
}
QTabBar::tab:top {
border-bottom-right-radius: 0;
border-bottom-left-radius: 0;
border-bottom: 1px solid transparent;
margin: 3px 3px 0 0;/*lr margin*/
}

QTabBar::tab:bottom {
border-top-right-radius: 0px;
border-top-left-radius: 0px;
border-top: 1px solid transparent;
margin: 0 2px 3px 0;/*lr margin*/
}

QTabBar::tab:right {
border-top-right-radius: 0px;
border-bottom-right-radius: 0px;
border-right: 1px solid transparent;
margin: 0 0 0px 0px;/*2, 3*/
}

QTabBar::tab:left {
border-top-left-radius: 0px;
border-bottom-left-radius: 0px;
border-left: 1px solid transparent;
margin: 0 0px 0px 0;/*3, 2*/
}

QTabBar::tab:selected {
color: #2ECCFA;
}

QTabBar::tab:top:hover {
border-bottom-color: #49A2BB/*0F82A3*/;
}
