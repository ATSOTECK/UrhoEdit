border-right-color: #2ECCFA;
}
QTabBar::tab:left:selected,
QTabBar::tab:left:hover {
border-left-color: #2ECCFA;
}
*/

QTabBar::tab:hover,
QTabBar::tab:focus {
color: #fff;
}

